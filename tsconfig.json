{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "dom"],
    "types": ["cypress", "node"],
    "moduleResolution": "node",
    "module": "ES2022",
    "strict": true,
    "noImplicitAny": true,
    "baseUrl": ".",
    "paths": {
      "@types/*": ["cypress/types/*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noEmit": true
  },
  "include": ["cypress/**/*.ts", "cypress.config.ts"],
  "exclude": ["node_modules"]
}
